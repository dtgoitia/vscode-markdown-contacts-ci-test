{
  "name": "markdown-contacts-ci-tests",
  "displayName": "markdown-contacts-ci-tests",
  "description": "",
  "publisher": "dtgoitia",
  "version": "0.0.5",
  "license": "MIT",
  "keywords": [
    "markdown",
    "contacts"
  ],
  "engines": {
    "vscode": "^1.56.0"
  },
  "repository": {
    "url": "https://github.com/dtgoitia/vscode-markdown-contacts.git"
  },
  "bugs": {
    "url": "https://github.com/dtgoitia/vscode-markdown-contacts/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Markdown contacts",
      "properties": {
        "markdown-contacts.globalNames": {
          "type": "array",
          "default": [],
          "description": "Names to be suggested, e.g.: PabloPicasso. Available in all workspaces"
        },
        "markdown-contacts.workspaceNames": {
          "type": "array",
          "default": [],
          "description": "Names to be suggested, e.g.: PabloPicasso. Available only in one workspace"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vsce": "vsce",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^9.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vsce": "^1.93.0",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
